{
    "name": "production-project",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "webpack serve",
        "start:vite": "vite",
        "start:dev": "concurrently \"npm run start\" \"npm run start:dev:server\"",
        "start:dev:vite": "concurrently \"npm run start:vite\" \"npm run start:dev:server\"",
        "start:dev:server": "node ./json-server/index.js development",
        "build:prod": "webpack --env mode=production",
        "build:dev": "webpack --env mode=development --env port=3000",
        "analyze:dev": "webpack --env analyze=true --env mode=development",
        "analyze:prod": "webpack --env analyze=true --env mode=production",
        "lint:ts": "eslint \"**/*.{ts,tsx}\"",
        "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
        "prettier": "prettier --write \"**/*.{ts,tsx,json}\"",
        "lint:scss": "npx stylelint \"**/*.scss\"",
        "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
        "test:e2e": "cypress open",
        "test:unit": "jest --config ./config/jest/jest.config.ts",
        "test:ui": "loki test",
        "test:ui:ci": "npx loki --requireReference --reactUri file:./storybook-static",
        "test:ui:approve": "npx loki approve",
        "test:ui:report": "npm run test:ui:json && npm run test:ui:html",
        "test:ui:json": "node scripts/generate-visual-json-report.js",
        "test:ui:html": "reg-cli --from .loki/report.json --report .loki/report.html",
        "storybook": "start-storybook -p 6006 -c ./config/storybook",
        "build:storybook": "build-storybook -c ./config/storybook",
        "prepare": "husky install",
        "generate:slice": "ts-node scripts/createSlice/index.js",
        "postinstall": "ts-node scripts/removeCache.ts",
        "remove-feature": "npx ts-node scripts/removeFeatures.ts"
    },
    "lint-staged": {
        "**/*.{ts,tsx}": [
            "eslint"
        ]
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
